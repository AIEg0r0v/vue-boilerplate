.tags {
  display: flex;
  flex-flow: wrap row;
  margin-bottom: 35px;
}
.tags__items {
  background: #F3F6F9;
  border-radius: 40px;
  padding: 14px 22px 14px 22px;
  margin-right: 8px;

  &:last-child {
    margin-right: 0px;
  }
}
.tag {
  font-weight: 600;
  font-size: 15px;
  color: rgba(70, 78, 98, 0.8);
}
